public class Main {

	public static void main(String[] args) {
		int number = 5;
		for(int multiplier = 1; multiplier <= 10; multiplier++) {
			int result = number * multiplier;
			System.out.println(number + " x " + multiplier + " = " + result);
		}	
	}
}
