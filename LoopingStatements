package forloop;

public class MathFiveTable {

	public static void main(String[] args) {
		int a = 1;
		int j = 5;
		for(int i =5;i<=50;i=i+5) {
			System.out.println(j + " x " + a + " = " + i);
			a++;
			
		}

	}

}
